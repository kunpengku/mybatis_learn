<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ClipMapper">
    <select id="selectClip" resultType="mybatis_learn.pojo.Clip">
        select *
        from entity_res_clip where id = #{id}
    </select>
</mapper>

